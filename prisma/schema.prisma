datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
  binaryTargets = ["native", "linux-musl-openssl-3.0.x"]
}

model Issue {
  id        Int      @id @default(autoincrement())
  rawText   String   @db.Text
  cleanText String   @db.Text
  cluster   Int
  createdAt DateTime @default(now())
}

model Seminar {
  id          Int     @id @default(autoincrement())
  title       String
  cluster     Int
}

model Materi {
  id        Int      @id @default(autoincrement())
  cluster   Int
  question  String
  answer    String
  keywords  Json
  link      String?

  createdAt DateTime @default(now())
}